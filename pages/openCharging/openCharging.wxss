/* 头部 */

.head {
  background: #fff;
  width: 100%;
  padding: 48rpx 0 20rpx 0;
}

.head_title {
  font-size: 28rpx;
  color: #333;
  padding: 0 24rpx 12rpx 24rpx;
}

.head_time {
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  flex-direction: row;
}

.head_time button {
  flex: 1;
}

.time {
  position: relative;
  width: 218rpx;
}

.time text:nth-child(1) {
  font-size: 38rpx;
  line-height: 38rpx;
}

.time text:nth-child(2) {
  font-size: 20rpx;
  line-height: 20rpx;
  margin-top: 10rpx;
}

.cancel {
  position: absolute;
  line-height: 90rpx;
  left: 120rpx;
  color: #0da297;
}
.cancel > text {
  font-size: 25rpx;
  border: 1rpx solid #0da297;
  border-radius: 999px;
  padding: 8rpx;
}

.sitTime {
  border: 2rpx solid #0da297;
  height: 120rpx;
  width: 218rpx;
  border-radius: 8rpx;
  background: #fff;
  text-align: center;
  color: #0da297;
  font-size: 38rpx;
  margin-bottom: 24rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}

