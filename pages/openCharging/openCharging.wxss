  text-align: right;
}

picker {
  width: 520rpx;
  text-align: right;
}

.ordinary_picker {
  width: 270rpx;
  text-align: center;
  color: #999999;
  display: inline-block;
  height: 88rpx;
  background: #fff;
  line-height: 88rpx;
  font-size: 36rpx;
}


/* 中间下 */

.middle_bottom {
  display: flex;
  padding: 30rpx 25rpx;
  margin-bottom: 45rpx;
  border-bottom: 2rpx solid #e5e5e5;
}

.middle_bottom view {
  flex: 1;
}

.middle_bottom view:nth-child(2) {
  text-align: right;
  color: #666;
  font-size: 24rpx;
  line-height: 48rpx !important;
}

.middle_bottom .details {
  line-height: 34rpx;
  text-align: right;
}

.middle_bottom view:nth-child(2) button {
  width: 100rpx;
  height: 40rpx;
  background: rgba(13, 162, 151, 1);
  border-radius: 20rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  margin-left: 283rpx;

}

.middle_bottom view {
  flex: 1;
}

.middle_bottom:nth-last-child(1) {
  border: 0;
}

/* 底部 */

.bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  display: flex;
  justify-content: space-around;
}

.bottom .openCharging {
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  background: #0da297;
  color: #fff;
  font-size: 36rpx;
  border-radius: 0;
  font-family: YouYuan;
  border-right: 1px solid #999999;
}

.grey {
  background: #dfdfdf;
  color: #999;
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  font-size: 36rpx;
  border-radius: 0;
  font-family: YouYuan;
}
.bottom .month_btn {
  width: 100%;
  height: 98rpx;
  line-height: 98rpx;
  background: #0da297;
  color: #fff;
  font-size: 36rpx;
  border-radius: 0;
  font-family: YouYuan;
  border-left: 1px solid #999999;
}

.alarm {
  color: red;
  font-size: 20rpx;
  position: fixed;
  bottom: 95rpx;
  width: 100%;
  background: white;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
}

/* 遮罩 */

.mask {
  background: #000;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 2;
  width: 100%;
  height: 100%;
  opacity: 0.5;
  overflow: hidden;
}



