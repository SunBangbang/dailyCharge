/* 占位 */

.box {
  background: #f4f4f4;
  height: 12rpx;
}

/* 中间 */

.middle {
  width: 100%;
  background: #fff;
  color: #333;
  font-size: 28rpx;
}

.middle_top {
  display: flex;
  padding: 0 25rpx;
  border-bottom: 2rpx solid #e5e5e5;
  line-height: 88rpx;
  justify-content: space-between;
}

.middle_top_ {
  display: flex;
  padding: 22rpx 25rpx;
  border-bottom: 2rpx solid #e5e5e5;
  justify-content: space-between;
}

.mxc {
  width: 550rpx;
  word-break: break-all;
}

.middle_top text {
  flex: 1;
}

.middle_top text:nth-child(2) {
  text-align: right;
  color: #666;
  font-size: 24rpx;

}

